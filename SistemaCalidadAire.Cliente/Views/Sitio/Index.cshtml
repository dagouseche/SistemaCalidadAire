<script type="text/javascript">

    function OnFailure(e) {
        $("#mensaje").empty().append("<label>Usuario o contraseña incorrecto.</label>");
    }

    function OnBegin() {
        $("#mensaje").empty().append("<img src='Content/img/loading.gif'></img>");
    }

    function myfunction() {

        var validator = $("#formulario").validate();
        if (!validator.form()) {
            return;
        }

        var formData = new FormData(document.getElementById("formulario"));

        $.ajax({
            url: "inicio/enviarCorreo",
            data: formData,
            type: "post",
            contentType: false,
            processData: false,
            success: function (data) {
                AlertaConfirmacion("Envio Exitoso", "", "success");
                $("#nombre").val("");
                $("#correo").val("");
                $("#asunto").val("");
                $("#mensaje").empty();

            },
            error: function (error) {
                AlertaConfirmacion("Envio Incorrecto", "", "error");
            }
        })
    }

    function AlertaConfirmacion(encabezado, mensaje, tipo) {
        swal({
            title: encabezado,
            text: mensaje,
            type: tipo,
            showCancelButton: false,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '',
            confirmButtonText: 'OK',
            cancelButtonText: '',
            confirmButtonClass: 'btn btn-success',
            cancelButtonClass: '',
            buttonsStyling: false
        }).then(function () { })
    }

    function ModalLogin() {
        $("#modalLogin").modal('show');
    }
</script>
